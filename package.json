{
  "name": "@cerberustech/react-cli",
  "version": "0.2.7",
  "description": "CLI tool for React component creation",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "react-cli": "dist/index.js"
  },
  "dependencies": {
    "config": "^3.3.6",
    "coveralls": "^3.1.0",
    "dedent": "^0.7.0",
    "log4js": "^6.3.0",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.39",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.15",
    "@types/log4js": "^2.3.5",
    "@types/mkdirp": "^1.0.1",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.14.9",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:coveralls": "jest --coverage --passWithNoTests --coverageReporters=text-lcov | coveralls",
    "build": "tsc",
    "start:prod:win": "npm run build && node ./dist/index.js",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:fix"
    ]
  },
  "author": "Akos Szabacsi",
  "license": "MIT"
}
